    "@angular/forms": "~11.2.7",
    "@angular/material": "^11.2.8",
    "@angular/platform-browser": "~11.2.7",
    "@angular/platform-browser-dynamic": "~11.2.7",
    "@angular/router": "~11.2.7",
    "@angular/youtube-player": "^11.2.8",
    "@types/resize-observer-browser": "^0.1.5",
    "bootstrap": "^4.6.0",
    "jquery": "^3.6.0",
